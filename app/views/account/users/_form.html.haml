.card-content
  .form-group
    = simple_form_for [:account, @user] do |f|
      = f.input :first_name, input_html: { max_length: 50 }
      = f.input :last_name, input_html: { max_length: 50 }
      = f.input :email    
      = f.button :submit, "Submit", class: "btn btn-primary waves-effect waves-light"        
        
          